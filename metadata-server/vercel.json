{
  "version": 2,
  "builds": [
    {
      "src": "metadata-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "metadata-server.js"
    }
  ],
  "functions": {
    "metadata-server.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "regions": ["fra1", "cdg1"],
  "headers": [
    {
      "source": "/(.*)\\.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=3600"
        }
      ]
    }
  ]
}